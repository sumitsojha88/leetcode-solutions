class Solution {
public:
    string restoreString(string s, vector<int>& in) {
              string ans="";
            for(int i=0;i<s.size();i++)
            {
                   ans += s[i];
            }
            for(int i=0;i<in.size();i++)
            {
                   ans[in[i]] = s[i];
            }
           
            
            return ans;
    }
};
