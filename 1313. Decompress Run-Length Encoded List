class Solution {
public:
    vector<int> decompressRLElist(vector<int>& nums) {
        vector<int> R;
        
            for(int i=1;i<nums.size();i=i+2)
            {
                    int num=nums[i-1];
                    int value=nums[i];
                    for(int i=0;i<num;i++)
                    {
                              R.push_back(value);
                    }
            }
            return R;
    }
};
