class Solution {
public:
    ListNode *detectCycle(ListNode *head) {
         ListNode *fast, *slow;
          fast = slow = head;
        while (fast && fast->next) {
        fast = fast->next->next;
        slow = slow->next;
        if (fast == slow) {
            slow = head;
            while (slow != fast) {
                slow = slow->next;
                fast = fast->next;
            }
            return slow;
        }
    }
    return NULL;
    }
};
