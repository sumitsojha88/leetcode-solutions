class Solution {
public:
        void Traverse(TreeNode* root,vector<int>& v){
            if(root!=NULL)
            {
                Traverse(root->left,v);
               Traverse(root->right,v);
                 v.push_back(root->val);   
             }
       }  
        
        
    vector<int> postorderTraversal(TreeNode* root) {
        vector<int> v;
            Traverse(root,v );
            
            return v;
    }
};
