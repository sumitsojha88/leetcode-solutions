class Solution {
public:
vector<int> preorderTraversal(TreeNode* root) {
        vector<int> v;
        stack<TreeNode*> s;
            while(root!=NULL || !s.empty())
            {
                    while(root!=NULL)
                    {
                            v.push_back(root->val);
                           s.push(root);
                         root=root->left;
                    }
                    
                    root=s.top();
                    s.pop();
                    
                    root=root->right;
            }
        return v;
    }
   
};
